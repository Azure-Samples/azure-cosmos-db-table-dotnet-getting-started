<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>

      <add key="PremiumStorageConnectionString"
          value="DefaultEndpointsProtocol=https;AccountName=MYSTORAGEACCOUNT;AccountKey=AUTHKEY;TableEndpoint=https://COSMODB.documents.azure.com" />
      <add key="StandardStorageConnectionString"
          value="DefaultEndpointsProtocol=https;AccountName=MYSTORAGEACCOUNT;AccountKey=AUTHKEY;EndpointSuffix=core.windows.net" />
      
      <!--See https://aka.ms/premiumtables for configurations -->
      <add key="TableThroughput" value="400"/>
      <add key="TableConsistencyLevel" value="Session"/>
      <add key="TablePreferredLocations" value="West US,East US"/>
    </appSettings>
</configuration>